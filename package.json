        "ora": "^1.3.0",
        "table": "^4.0.2"
    },
    "devDependencies": {
        "eslint": "^4.4.1",
        "eslint-config-piecioshka": "^2.0.0",
        "istanbul": "^0.4.5",
        "jasmine": "^2.7.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/piecioshka/makiwara.git"
